{
  "name": "@ski/mixins",
  "version": "0.1.0",
  "description": "custom elements javascript class extensions",
  "module": "mixins.js",
  "main": "mixins.min.js",
  "types": ".types/index.d.ts",
  "scripts": {
    "build": "run-s clean build:*",
    "watch": "run-p \"build:* -- --watch\"",
    "clean": "rimraf js .types",
    "build:bundle": "rollup -c",
    "build:typescript": "ttsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saviski/ski-mixins.git"
  },
  "keywords": [
    "custom",
    "elements",
    "web",
    "components",
    "class",
    "mixin",
    "typescript",
    "javascript"
  ],
  "author": "saviski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saviski/ski-mixins/issues",
    "email": "marcelo@saviski.dev"
  },
  "homepage": "https://github.com/saviski/ski-mixins#readme",
  "dependencies": {
    "param-case": "^3.0.3"
  },
  "prettier": {
    "printWidth": 90,
    "semi": false,
    "tabWidth": 2,
    "proseWrap": "never",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "singleQuote": true
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.4",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "files": [
    ".types",
    "js",
    "ski-mixins.min.js",
    "ski-mixins.min.js.map"
